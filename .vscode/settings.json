{
  // Mainly for static files
  "liveServer.settings.useWebExt": true,

  // This means that you change your real URL (current PHP url) 
  // to another URL (which Live Sever starts).
  "liveServer.settings.proxy": {
      "enable": true,                             //   i. enabled
      "baseUri": "php-nodestudios.com/",                             //  ii. workspace
      "proxyUri": "http://php-nodestudios.com" // iii. actual address
  },
  "liveServer.settings.multiRootWorkspaceName": "php-nodestudios.com",
  "liveServer.settings.port": 5600,
}